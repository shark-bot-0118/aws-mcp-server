# AWS認証情報の設定例
# このファイルを.envにコピーして、実際の値を設定してください
# 注意: .envファイルはGitにコミットしないでください！

# AWS認証情報 (必須)
AWS_ACCESS_KEY_ID=AKIAXXXXXXXXXXXXXXXX
AWS_SECRET_ACCESS_KEY=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# デフォルト設定 (オプション)
AWS_DEFAULT_REGION=ap-northeast-1
AWS_DEFAULT_PROFILE=default
AWS_REGION=ap-northeast-1

# サーバー設定
MCP_HOST=0.0.0.0
MCP_PORT=8000
LOG_LEVEL=INFO
LOG_FILE=aws_cli_mcp.log
# 破壊的操作の自動承認 (デフォルト: false)
# trueにすると、破壊的操作（削除など）実行時の確認トークン入力をスキップします。
AWS_MCP_AUTO_APPROVE_DESTRUCTIVE=false

# ストレージ設定
SQLITE_PATH=./data/aws_mcp.sqlite
ARTIFACT_PATH=./data/artifacts

# ポリシー設定
POLICY_PATH=./policy.yaml

# Smithyモデル設定
# AWS公式の api-models-aws を使用する場合は以下を設定
SMITHY_SYNC_URL=https://github.com/aws/api-models-aws.git
SMITHY_SYNC_REF=main
SMITHY_CACHE_PATH=./data/smithy_cache
SMITHY_MODEL_PATH=./data/smithy_cache/models
SMITHY_AUTO_SYNC=true
TOOL_REGISTRY_MODE=auto
TOOL_REGISTRY_MAX_TOOLS=200
# 自動同期を無効化する場合:
# SMITHY_AUTO_SYNC=false
# ツール数が多すぎる場合は core モード:
# TOOL_REGISTRY_MODE=core
# SMITHY_ALLOWLIST_SERVICES=ec2,iam
# SMITHY_ALLOWLIST_OPERATIONS=ec2:RunInstances,iam:CreateRole
# SMITHY_ON_DEMAND_ONLY=false

